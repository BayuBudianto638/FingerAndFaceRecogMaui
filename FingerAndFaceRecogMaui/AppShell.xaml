<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="FingerAndFaceRecogMaui.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:FingerAndFaceRecogMaui.Views"
	xmlns:local="clr-namespace:FingerAndFaceRecogMaui">

	<TabBar>
		<ShellContent
			Title="Main"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:MainPage}" />
		<ShellContent
			Title="Blank"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:BlankPage}" />
		<ShellContent
			Title="ListDetail"
			Icon="iconlistdetail.png"
			ContentTemplate="{DataTemplate views:ListDetailPage}" />
		<ShellContent
			Title="Map"
			Icon="iconmap.png"
			ContentTemplate="{DataTemplate views:MapPage}" />
		<ShellContent
			Title="Sample"
			Icon="iconsample.png"
			ContentTemplate="{DataTemplate views:SamplePage}" />
	</TabBar>
</Shell>
